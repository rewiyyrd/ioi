#include <stdio.h>

int main() {
    float side, area, perimeter;

    printf("Введіть довжину сторони квадрата: ");
    scanf("%f", &side);

    area = side * side;
    perimeter = 4 * side;

    printf("Площа квадрата: %f\n", area);
    printf("Периметр квадрата: %f\n", perimeter);

    return 0;
}
