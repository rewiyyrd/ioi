#include <stdio.h>

int main() {
    float vartist, kilkist, vartist_odunutsi;

    printf("Vvedit vartist tovaru: ");
    scanf("%f", &vartist);

    printf("Vvedit kilkist tovaru: ");
    scanf("%f", &kilkist);

    vartist_odunutsi = vartist / kilkist;

    printf("Vartist odunutsi tovaru: %.6f\n", vartist_odunutsi);

    return 0;
}
