#include <stdio.h>

#define ROWS 3
#define COLS 3

int main() {
    int arr[ROWS][COLS], i, j, k, temp;

    // введення елементів в масив з клавіатури
    printf("Введіть елементи масиву>>>\n");
    for (i = 0; i < ROWS; i++) {
        for (j = 0; j < COLS; j++) {
            printf("Елемент [%d][%d]>>> ", i, j);
            scanf("%d", &arr[i][j]);
        }
    }

    // сортування елементів масиву за зростанням
    for (i = 0; i < ROWS; i++) {
        for (j = 0; j < COLS; j++) {
            for (k = j + 1; k < COLS; k++) {
                if (arr[i][j] > arr[i][k]) {
                    // міняємо місцями елементи
                    temp = arr[i][j];
                    arr[i][j] = arr[i][k];
                    arr[i][k] = temp;
                }
            }
        }
    }

    // виведення відсортованих елементів
    printf("\nВідсортовані елементи масиву>>>\n");
    for (i = 0; i < ROWS; i++) {
        for (j = 0; j < COLS; j++) {
            printf("%d ", arr[i][j]);
        }
        printf("\n");
    }

    return 0;
}
