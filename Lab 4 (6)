#include <stdio.h>

int main() {
    int a, b, c;
    printf("Введіть три числа>>> ");
    scanf("%d %d %d", &a, &b, &c);

    if (a > b) {
        if (a > c) {
            printf("%d найбільше число\n", a);
        } else {
            printf("%d є найбільше число\n", c);
        }
    } else {
        if (b > c) {
            printf("%d є найбільше число\n", b);
        } else {
            printf("%d є найбільше число\n", c);
        }
    }

    return 0;
}
